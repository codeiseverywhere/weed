                          <img src="{{ section.settings.star | img_url: 'master' }}" class="pd__accordion-star">
                          <img src="{{ section.settings.star | img_url: 'master' }}" class="pd__accordion-star">
                        {% elsif section.settings.rv-stars-1 == 5 %}
